{
  "name": "@netsells/nuxt-non-pojo",
  "version": "1.0.0",
  "description": "A way to use non-POJO class instances in NuxtJS",
  "main": "dist/module.js",
  "scripts": {
    "lint": "eslint src --max-warnings=0",
    "test": "jest",
    "build": "babel src -d dist",
    "prepublishOnly": "yarn build"
  },
  "author": "Sam Boylett <sam.boylett@netsells.co.uk>",
  "license": "ISC",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.vue$": "vue-jest",
      ".*\\.js$": "babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "dependencies": {
    "consola": "^2.11.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@netsells/eslint-config": "^2.4.1",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "eslint": "^4.10.0",
    "eslint-plugin-jquery": "^1.5.1",
    "eslint-plugin-require-jsdoc-except": "^1.3.0",
    "jest": "^25.1.0",
    "vue": "^2.6.11",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.2"
  }
}
